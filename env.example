# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://olvaro.net

# AI Provider Configuration
# For workflow extraction (smart selection based on document size)
AI_PRIMARY_PROVIDER=openai
AI_FALLBACK_PROVIDER=gemini

# For other tasks (synthesis, planning, deduplication) - legacy
AI_PROVIDER=claude

# API Keys
OPENAI_API_KEY=sk-proj-your-openai-api-key-here
GOOGLE_AI_API_KEY=your-google-ai-api-key-here
ANTHROPIC_API_KEY=sk-ant-api03-your-claude-api-key-here  # Keep for backward compatibility (other tasks)

# Inngest Configuration (optional for local development)
INNGEST_EVENT_KEY=your-inngest-key-here
INNGEST_SIGNING_KEY=your-inngest-signing-key-here

# Cloud Storage OAuth Configuration
# Google Drive
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/import/googledrive/auth/callback

# Microsoft (OneDrive/SharePoint)
MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret
MICROSOFT_REDIRECT_URI=http://localhost:3000/api/import/onedrive/auth/callback
MICROSOFT_TENANT_ID=common

# Dropbox
DROPBOX_APP_KEY=your-dropbox-app-key
DROPBOX_APP_SECRET=your-dropbox-app-secret
DROPBOX_REDIRECT_URI=http://localhost:3000/api/import/dropbox/auth/callback

# Token Encryption (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
TOKEN_ENCRYPTION_KEY=your-32-byte-hex-encryption-key

# Feature Flags
# Enable agentic chat features (tree modifications via AI)
# Default: false - agentic features are disabled
ENABLE_AGENTIC_CHAT=false

# Enable multi-pass extraction (recommended for large documents)
# Multi-pass: Discovery → Targeted Extraction → Verification
# Benefits: 30% cheaper, 80% better quality (9/10 vs 5/10)
# Default: false - single-pass extraction (faster, lower quality)
ENABLE_MULTI_PASS_EXTRACTION=false